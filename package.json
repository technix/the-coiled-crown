{
  "name": "@atrament/the-coiled-crown",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prestart": "node node_modules/inkjs/dist/inkjs-compiler.js -o src/assets/game/the-coiled-crown.ink.json game/the-coiled-crown.ink",
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "prebuild": "node node_modules/inkjs/dist/inkjs-compiler.js -o src/assets/game/the-coiled-crown.ink.json game/the-coiled-crown.ink",
    "build": "preact build --no-esm --no-prerender",
    "serve": "sirv build --port 8900 --cors --single",
    "dev": "preact watch --port 8900",
    "analyze": "preact build --analyze --no-esm --no-prerender",
    "lint": "eslint src"
  },
  "dependencies": {
    "@atrament/web": "1.0.0",
    "@nanostores/preact": "0.5.0",
    "history": "5.3.0",
    "inkjs": "2.2.2",
    "normalize.css": "8.0.1",
    "preact": "10.17.1",
    "preact-router": "4.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.15",
    "eslint": "8.49.0",
    "eslint-config-preact": "1.3.0",
    "eslint-plugin-compat": "4.2.0",
    "if-env": "1.0.4",
    "jest": "^29.7.0",
    "preact-cli": "3.5.0",
    "sirv-cli": "1.0.14"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
